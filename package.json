{
  "name": "root",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "postinstall": "if [ \"$NODE_ENVIRONMENT\" != \"production\" ]; then husky; fi",
    "start": "lerna run start --parallel",
    "build": "lerna run build --parallel",
    "build-sourcemaps": "lerna run build-sourcemaps --parallel",
    "build-and-serve": "npm run build && concurrently \"lerna run serve --parallel\"",
    "serve": "lerna run serve --parallel",
    "test": "jest --config jest.config.js",
    "jest-clear-cache": "jest --clearCache",
    "jest-test-config": "jest --config jest.config.js --showConfig > jest-config-output.json",
    "tsc": "lerna run tsc --parallel",
    "lint": "lerna run lint --parallel",
    "format": "lerna run format --parallel",
    "knip": "knip"
  },
  "dependencies": {
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@fontsource/roboto": "5.1.0",
    "@mui/material": "6.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.2.0",
    "@reduxjs/toolkit": "2.8.2",
    "redux-saga": "1.3.0"
  },
  "devDependencies": {
    "@module-federation/enhanced": "0.15.0",
    "@rsdoctor/rspack-plugin": "1.1.3",
    "@rspack/cli": "1.3.15",
    "@rspack/core": "1.3.15",
    "@rspack/dev-server": "1.1.3",
    "@rspack/plugin-react-refresh": "1.4.3",
    "react-refresh": "0.17.0",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "@types/react-redux": "7.1.34",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "concurrently": "9.1.0",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "@eslint/js": "9.21.0",
    "eslint": "9.21.0",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "express": "4.21.2",
    "husky": "9.1.7",
    "knip": "5.45.0",
    "lerna": "8.2.1",
    "lint-staged": "15.2.11",
    "prettier": "3.5.3",
    "typescript": "5.7.2",
    "tsc-files": "1.1.4",
    "@types/node": "24.0.1"
  }
}
