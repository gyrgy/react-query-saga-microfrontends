{
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "es5",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "lib": ["es2016", "ES2017", "dom", "ES2021"],
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
